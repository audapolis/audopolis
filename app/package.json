{
  "name": "audapolis",
  "productName": "audapolis",
  "version": "0.2.1",
  "description": "an editor for spoken-word audio with automatic transcription",
  "author": "audapolis team <audapolis@pajowu.de>",
  "maintainers": [
    "pajowu <git@ca.pajowu.de>",
    "anuejn <jarohabiger@googlemail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/audapolis/audapolis",
    "directory": "app/"
  },
  "main": "build/main_process/start.cjs.js",
  "scripts": {
    "prebuild": "node scripts/generate_licenses.mjs generated/licenses.zip server/licenses.md",
    "build": "node scripts/build.js",
    "predist": "cross-env MODE=production npm run build",
    "dist": "electron-builder build --config electron-builder.config.js",
    "test": "npm-run-all --continue-on-error test:*",
    "test:fast": "jest --testPathIgnorePatterns='.*\\.pup\\.spec.ts'",
    "test:puppeteer": "if which xvfb-run >/dev/null 2>&1; then xvfb-run -a jest '.*\\.pup\\.spec.ts'; else jest '.*\\.pup\\.spec.ts'; fi",
    "start": "node scripts/dev.js",
    "check": "npm-run-all --continue-on-error check:*",
    "check:tsc": "tsc --noEmit --incremental",
    "check:eslint": "eslint --ext .ts,.tsx . --cache",
    "fmt": "prettier --write \"{src,main_process}/**/*\""
  },
  "keywords": [],
  "license": "AGPL-3.0",
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@swc/core": "^1.3.37",
    "@swc/jest": "^0.2.24",
    "@types/electron-devtools-installer": "^2.2.2",
    "@types/jest": "^29.4.0",
    "@types/lodash": "^4.14.191",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/redux": "~3.6.0",
    "@types/styled-components": "^5.1.26",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "cross-env": "^7.0.3",
    "electron": "~23.1.2",
    "electron-builder": "^24.0.0",
    "esbuild": "^0.17.11",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.35.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.4.3",
    "nlf": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "portfinder": "^1.0.32",
    "prettier": "^2.8.4",
    "puppeteer-core": "^19.7.2",
    "tmp": "^0.2.1",
    "tree-kill": "^1.2.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-commonjs-externals": "^0.1.1"
  },
  "dependencies": {
    "@audapolis/webvtt-writer": "^1.0.5",
    "@reactour/mask": "~0.4.0",
    "@reactour/utils": "~0.2.1",
    "@reduxjs/toolkit": "^1.9.3",
    "@tedconf/fessonia": "^2.2.1",
    "appdirectory": "^0.1.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.0.0",
    "evergreen-ui": "^7.1.4",
    "ffmpeg-static": "^5.1.0",
    "glob": "^9.2.1",
    "immer": "^9.0.19",
    "jest-environment-jsdom": "^29.5.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "rc-progress": "^3.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.4.0",
    "react-media-hook": "^0.5.0",
    "react-redux": "~7.2.9",
    "redux-undo": "^1.0.1",
    "styled-components": "^5.3.8",
    "uuid": "^9.0.0"
  },
  "optionalDependencies": {
    "dmg-license": "^1.0.9"
  }
}
